<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.umpbizgo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Customer.UserProfileActivity"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".SellerLoginRegister.SellerRegisterActivity" />
        <activity android:name=".SellerLoginRegister.SellerLoginActivity" />
        <activity android:name=".CustomerLoginRegister.CustomerRegisterActivity" />
        <activity android:name=".CustomerLoginRegister.CustomerLoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>